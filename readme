$ ./gradlew build
$ docker build -t fadi/bootcamp .
$ docker-compose up --scale bootcamp=3
$ docker-compose down


docker run -p 8080:8080 fadi/bootcamp

$ docker image prune --all
$ docker-compose --compatibility up --build

